{
  "name": "node-express-mongodb-jwt-rest-api-skeleton",
  "version": "9.0.5",
  "description": "Imagine and create",
  "scripts": {
    "start": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register ./src/index.ts",
    "mocha": "nyc mocha --timeout=5000 --exit",
    "test": "pnpm run coverage:clean && pnpm run test:unit && pnpm run test:e2e && pnpm run coverage",
    "test:unit": "cross-env NODE_ENV=test jest --coverage",
    "test:e2e": "cross-env NODE_ENV=test pnpm run fresh && pnpm run mocha",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --pretty --transpile-only -r tsconfig-paths/register ./src/index.ts",
    "tsc": "npx tsc",
    "postinstall": "pnpm run tsc",
    "fresh": "pnpm run clean && pnpm run seed",
    "clean": "node clean.js",
    "prettier": "prettier --check \"**/*.{js,json,ts,md}\"",
    "prettier:fix": "prettier --write \"**/*.{js,json,ts,md}\"",
    "swagger": "pnpm ts-node ./swagger.ts",
    "lint": "eslint --fix \"src/**/*.{js,ts}\"",
    "remark": "remark . -o",
    "coverage": "npm run coverage:merge && npm run coverage:merge-report",
    "coverage:clean": "rm -rf .nyc_output && rm -rf coverage",
    "coverage:merge": "istanbul-merge --out coverage/merged/coverage-final.json ./coverage/unit/coverage-final.json ./coverage/e2e/coverage-final.json",
    "coverage:merge-report": "nyc report --reporter=lcov --reporter=text --reporter=json --temp-dir=./coverage/merged --report-dir=./coverage/merged",
    "health": "node scripts/test-health-checks.js",
    "health:check": "curl -s http://localhost:3000/api/health | jq",
    "health:ready": "curl -s http://localhost:3000/api/health/ready | jq",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio"
  },
  "nyc": {
    "reporter": [
      "json",
      "text",
      "lcov"
    ],
    "report-dir": "coverage/e2e",
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "**/*.test.js",
      "jest.config.js",
      "**/data/**",
      "**/node_modules/**",
      "**/.history/**",
      "**/test/**",
      "**/coverage/**",
      "**/tmp/**"
    ],
    "all": true
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.0.0",
    "@google-cloud/logging-winston": "^6.0.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "axios": "^1.3.4",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv-safe": "^9.1.0",
    "ejs": "^3.1.10",
    "expeditious-engine-redis": "^0.1.2",
    "express": "^5.1.0",
    "express-expeditious": "^5.1.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "i18n": "^0.15.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.0",
    "mongoose-aggregate-paginate-v2": "^1.1.4",
    "mongoose-delete": "^0.5.4",
    "mongoose-paginate-v2": "^1.7.1",
    "mongoose-unique-validator": "^3.1.0",
    "morgan": "^1.10.0",
    "node-telegram-bot-api": "^0.61.0",
    "nodemailer": "^6.9.1",
    "nodemailer-mailgun-transport": "^2.1.5",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "request-ip": "^3.3.0",
    "swagger-ui-express": "^5.0.1",
    "trim-request": "^1.0.6",
    "uuid": "^11.1.0",
    "validator": "^13.15.15",
    "winston": "^3.17.0",
    "zod": "^4.0.13",
    "@prisma/client": "^5.16.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/bcrypt": "^6.0.0",
    "@types/body-parser": "^1.19.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/i18n": "^0.13.12",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.1.0",
    "@types/node-telegram-bot-api": "^0.64.9",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/request-ip": "^0.0.41",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/validator": "^13.15.2",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "chai": "^5.2.1",
    "chai-http": "^5.1.2",
    "cross-env": "^10.0.0",
    "eslint": "^9.32.0",
    "faker": "^6.6.6",
    "husky": "^9.1.7",
    "istanbul-merge": "^2.0.0",
    "jest": "^30.0.5",
    "lint-staged": "^16.1.2",
    "mocha": "^11.7.1",
    "mongo-seeding": "^4.0.2",
    "nodemon": "^3.1.10",
    "nyc": "^17.1.0",
    "prettier": "^3.6.2",
    "remark-cli": "^12.0.1",
    "swagger-autogen": "^2.23.7",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "prisma": "^5.16.1"
  },
  "keywords": [
    "javascript",
    "api",
    "node",
    "express",
    "mongo",
    "mongodb",
    "jwt",
    "postman",
    "i18n",
    "jwt-authentication",
    "token",
    "eslint",
    "starter",
    "web",
    "app",
    "mongoose",
    "rest",
    "skeleton",
